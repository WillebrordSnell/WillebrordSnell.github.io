#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/R-blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_401c28336a927c2f88f5c5b7f9736499/node_modules/vuepress/bin/node_modules:/mnt/e/R-blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_401c28336a927c2f88f5c5b7f9736499/node_modules/vuepress/node_modules:/mnt/e/R-blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_401c28336a927c2f88f5c5b7f9736499/node_modules:/mnt/e/R-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/R-blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_401c28336a927c2f88f5c5b7f9736499/node_modules/vuepress/bin/node_modules:/mnt/e/R-blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_401c28336a927c2f88f5c5b7f9736499/node_modules/vuepress/node_modules:/mnt/e/R-blog/node_modules/.pnpm/vuepress@2.0.0-rc.24_@vuepr_401c28336a927c2f88f5c5b7f9736499/node_modules:/mnt/e/R-blog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vuepress@2.0.0-rc.24_@vuepr_401c28336a927c2f88f5c5b7f9736499/node_modules/vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../.pnpm/vuepress@2.0.0-rc.24_@vuepr_401c28336a927c2f88f5c5b7f9736499/node_modules/vuepress/bin/vuepress.js" "$@"
fi
